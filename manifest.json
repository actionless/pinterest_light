{
	"manifest_version": 2,
	"name": "pinterest_light",
	"version": "0.3",

	"browser_action": {
		"default_title": "Pinterest Button light",
		"default_icon": {
			"48": "icons/icon.png",
			"96": "icons/icon@2x.png"
		}
	},

	"permissions": ["activeTab", "tabs"],
	"background": {
		"scripts": ["background.js"]
	}

}
